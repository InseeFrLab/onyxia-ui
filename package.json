{
    "name": "onyxia-ui",
    "version": "0.0.0",
    "description": "A ui toolkit, like material-ui but with stronger typing and more opinionated",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/onyxia-ui.git"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/stories/",
        "dist/",
        "!dist/stories/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/garronej/onyxia-ui",
    "peerDependencies": {
        "react": "*"
    },
    "optionalDependencies": {
        "@types/react": "*"
    },
    "dependencies": {
        "evt": "beta",
        "tss-react": "^0.0.12",
        "powerhooks": "^0.0.39",
        "tsafe": "^0.4.0",
        "color": "3.1.3",
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.58",
        "memoizee": "^0.4.15"
    },
    "devDependencies": {
        "@types/memoizee": "^0.4.5",
        "@types/react": "^17.0.4",
        "react": "^16.14.0",
        "@types/color": "^3.0.1",
        "@types/node": "^15.3.1",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "evt": "^1.9.14",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "typescript": "^4.3.2",
        "copyfiles": "^2.4.1"
    }
}
