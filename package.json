{
    "name": "onyxia-ui",
    "version": "0.48.0",
    "description": "A uli toolkit, like material-ui but with stronger typing and more opinionated",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/onyxia-ui.git"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "build": "tsc && yarn copy-files",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 6006 --static-dir ./.storybook/static",
        "yarn_link": "ts-node src/bin/yarn_link.ts",
        "start_spa": "yarn yarn_link && cd src/test/spa && yarn start",
        "copy-files": "copyfiles -u 1 src/assets/**/*.svg src/assets/**/*.css src/assets/**/*.woff2 dist/"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/stories/",
        "!src/test/",
        "!src/bin/",
        "dist/",
        "!dist/stories/",
        "!dist/test/",
        "!dist/bin/",
        "!dist/package.json",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://ui.onyxia.dev",
    "peerDependencies": {
        "@emotion/react": "^11.9.0",
        "@mui/material": "^5.9.3",
        "@types/react": "^16.0.0 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "dependencies": {
        "@mui/icons-material": "^5.10.15",
        "color": "3.1.3",
        "evt": "^2.4.13",
        "memoizee": "^0.4.15",
        "minimal-polyfills": "^2.2.2",
        "powerhooks": "^0.20.31",
        "run-exclusive": "^2.2.18",
        "tsafe": "^1.4.1",
        "tss-react": "^4.4.4"
    },
    "devDependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@emotion/css": "^11.10.0",
        "@mui/material": "^5.10.3",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/addons": "^6.5.9",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",
        "@storybook/node-logger": "^6.5.9",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.9",
        "@storybook/theming": "^6.5.9",
        "storybook-dark-mode": "^1.1.0",
        "@types/color": "^3.0.1",
        "@types/memoizee": "^0.4.5",
        "@types/node": "^17.0.24",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-storybook": "^0.5.13",
        "eslint-plugin-tss-unused-classes": "^0.0.4",
        "husky": "^4.3.8",
        "i18nifty": "^1.3.6",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "ts-node": "^10.3.0",
        "typescript": "^4.7.4",
        "webpack": "^5.73.0",
        "react-markdown": "^5.0.3"
    }
}
