{
    "name": "onyxia-ui",
    "version": "4.2.3",
    "description": "The Onyxia UI toolkit",
    "repository": {
        "type": "git",
        "url": "git://github.com/InseeFrLab/onyxia-ui.git"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "prepare": "ts-node --skipProject scripts/prepare.ts",
        "build": "tsc && cpx \"src/assets/**/*.{svg,css,woff2}\" dist/assets/",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 6006 --static-dir ./.storybook/static",
        "link-in-web": "yarn build && ts-node --skipProject scripts/link-in-app.ts web",
        "start-test-app": "yarn build && ts-node --skipProject scripts/link-in-test-app.ts && cd test-app && rm -rf node_modules/.cache && yarn start"
    },
    "bin": {
        "copy-material-icons-to-public": "dist/bin/copy-material-icons-to-public.js"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/stories/",
        "dist/",
        "!dist/package.json",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "peerDependencies": {
        "@emotion/react": "^11.9.0",
        "@mui/material": "^5.9.3",
        "@types/react": "^16.0.0 || ^17.0.0 || ^18.0.0",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "dependencies": {
        "@mui/icons-material": "^5.14.18",
        "color": "3.1.3",
        "dompurify": "^3.0.6",
        "evt": "^2.5.3",
        "make-fetch-happen": "^10.2.1",
        "memoizee": "^0.4.15",
        "minimal-polyfills": "^2.2.3",
        "powerhooks": "^1.0.3",
        "run-exclusive": "^2.2.19",
        "tsafe": "^1.6.5",
        "tss-react": "^4.9.3",
        "yauzl": "^2.10.0",
        "yazl": "^2.5.1"
    },
    "devDependencies": {
        "@emotion/css": "^11.11.2",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.14.6",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/addons": "^6.5.9",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",
        "@storybook/node-logger": "^6.5.9",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.9",
        "@storybook/theming": "^6.5.9",
        "@types/color": "^3.0.3",
        "@types/dompurify": "^3.0.5",
        "@types/make-fetch-happen": "^10.0.3",
        "@types/memoizee": "^0.4.8",
        "@types/node": "^17.0.24",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/yauzl": "^2.10.2",
        "@types/yazl": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "cpx": "^1.5.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-storybook": "^0.5.13",
        "eslint-plugin-tss-unused-classes": "^1.0.2",
        "husky": "^4.3.8",
        "i18nifty": "^1.3.6",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^5.0.3",
        "react-scripts": "5.0.1",
        "storybook-dark-mode": "^1.1.0",
        "ts-node": "^10.3.0",
        "typescript": "^4.7.4",
        "webpack": "^5.73.0"
    }
}
