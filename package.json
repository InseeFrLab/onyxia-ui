{
    "name": "onyxia-ui",
    "version": "0.9.15",
    "description": "A uli toolkit, like material-ui but with stronger typing and more opinionated",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/onyxia-ui.git"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "copyfiles": "copyfiles -u 1 src/assets/**/* dist/",
        "build": "tsc && yarn copyfiles",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 6006 --static-dir ./.storybook/static",
        "link_test_projects_dependencies": "node dist/bin/link_test_projects_dependencies",
        "start": "yarn link_test_projects_dependencies && cd src/test && yarn start",
        "link_inhouse_deps": "ts-node --skip-project src/bin/link_inhouse_deps.ts"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/stories/",
        "!src/test/",
        "!src/bin/",
        "dist/",
        "!dist/stories/",
        "!dist/test/",
        "!dist/bin/",
        "!dist/package.json",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://ui.onyxia.dev",
    "peerDependencies": {
        "@material-ui/core": "^4.12.1",
        "react": "*"
    },
    "optionalDependencies": {
        "@material-ui/lab": "^4.0.0-alpha.58",
        "@types/react": "*",
        "tss-react": "^0.4.1"
    },
    "dependencies": {
        "@material-ui/icons": "^4.11.2",
        "color": "3.1.3",
        "evt": "2.0.0-beta.29",
        "memoizee": "^0.4.15",
        "powerhooks": "^0.7.1",
        "ts-toolbelt": "^9.6.0",
        "tsafe": "^0.4.1"
    },
    "devDependencies": {
        "@material-ui/core": "^4.12.1",
        "@material-ui/lab": "^4.0.0-alpha.58",
        "@storybook/addon-actions": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/addons": "^6.2.9",
        "@storybook/node-logger": "^6.2.9",
        "@storybook/preset-create-react-app": "^3.1.7",
        "@storybook/react": "^6.2.9",
        "@storybook/theming": "^6.2.9",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/color": "^3.0.1",
        "@types/memoizee": "^0.4.5",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "tss-react": "^0.4.1",
        "typescript": "4.2.2"
    }
}
